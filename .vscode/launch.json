{
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/as-new",
            "args": [
              "-o",
              "main.o",
              "main.s"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/c-test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
              {
                "description": "为 gdb 启用整齐打印",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ]
          },
    ]
}